{
    "name": "phespro/container",
    "description": "Super simple dependency injection container for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mschop",
            "email": "mschopdev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phespro\\Container\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "infection/infection": "^0.27"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
